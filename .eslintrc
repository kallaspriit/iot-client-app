{
    "extends": "sl",
    "rules": {
        "eol-last": "off",
        "no-console": "off",
        "no-continue": "off",
        "no-underscore-dangle": "off",
        "no-unused-vars": ["error", { "vars": "all", "args": "none" }],
        "react/prefer-stateless-function": "off",
        semi: ["error", "always"]
    },
    "globals": {
        "document": true,
        "window": true
    }
}
